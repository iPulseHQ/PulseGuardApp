# PulseGuard Native App Environment Variables
# Copy this file to .env and fill in your values

# Expo Project Configuration
EXPO_PUBLIC_PROJECT_ID=your-expo-project-id

# Backend API Configuration
EXPO_PUBLIC_API_URL=https://api.ipulse.one
EXPO_PUBLIC_API_TIMEOUT=10000

# App Configuration
EXPO_PUBLIC_APP_NAME=PulseGuard
EXPO_PUBLIC_APP_VERSION=3.0.0

# Clerk Authentication Configuration
# Get this from your Clerk dashboard
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_live_your_clerk_key

# App URLs
EXPO_PUBLIC_PULSEGUARD_URL=https://guard.ipulse.one
EXPO_PUBLIC_MAIN_WEBSITE_URL=https://ipulse.one
EXPO_PUBLIC_SUPPORT_URL=https://your-support-url